<!DOCTYPE html>
<html lang="en">
  <head>
    {{>boilerplate/blob-head}}
  </head>
  <body>
    <main id="{{pageId}}">
      {{#if pageHeader}}
        <h1 class="page-header">
          <a href="{{page.url}}">{{pageHeader}}</a>
        </h1>
      {{/if}}
      
      <div class="blob">
        {{{content}}}
      </div>
    </main>
    {{>boilerplate/analytics}}
  </body>
</html>